{
  "name": "Change Image",
  "version": "2.0",
  "description": "Change images on webpages that feature a certain string to different images.",
  "permissions": [
    "storage"
  ],
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "change.js"
      ]
    }
  ]
}